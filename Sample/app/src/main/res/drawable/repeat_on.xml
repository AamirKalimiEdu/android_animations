<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24"
            android:alpha="0.33">
            <path
                android:name="path_1"
                android:pathData="M 6 11 L 6 6 L 18 6 L 18 18 L 6 18 Z"
                android:strokeColor="#000000"
                android:strokeWidth="2"
                android:trimPathEnd="0.35"/>
            <path
                android:name="path_2"
                android:pathData="M 18 13 L 18 18 L 6 18 L 6 6 L 18 6 Z"
                android:strokeColor="#000000"
                android:strokeWidth="2"
                android:trimPathEnd="0.35"/>
            <group
                android:name="group"
                android:pivotX="19"
                android:pivotY="6"
                android:translateX="-1">
                <path
                    android:name="path_3"
                    android:pathData="M 17 7 L 17 10 L 21 6 L 17 2 L 17 5 Z"
                    android:fillColor="#000000"/>
            </group>
            <group
                android:name="group_1"
                android:pivotX="6"
                android:pivotY="18">
                <path
                    android:name="path_4"
                    android:pathData="M 7 17 L 7 14 L 3 18 L 7 22 L 7 19 Z"
                    android:fillColor="#000000"/>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="path_1">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="trimPathEnd"
                    android:duration="200"
                    android:valueFrom="0.35"
                    android:valueTo="0.85"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="trimPathStart"
                    android:duration="200"
                    android:valueFrom="0"
                    android:valueTo="0.5"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="path_2">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="trimPathEnd"
                    android:duration="200"
                    android:valueFrom="0.35"
                    android:valueTo="0.86"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="trimPathStart"
                    android:duration="200"
                    android:valueFrom="0"
                    android:valueTo="0.5"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="rotation"
                    android:duration="20"
                    android:valueFrom="0"
                    android:valueTo="90"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:startOffset="10"
                    android:duration="100"
                    android:valueFrom="0"
                    android:valueTo="12"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="rotation"
                    android:startOffset="100"
                    android:duration="20"
                    android:valueFrom="90"
                    android:valueTo="180"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="translateX"
                    android:startOffset="100"
                    android:duration="100"
                    android:valueFrom="-1"
                    android:valueTo="-14"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group_1">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="rotation"
                    android:duration="20"
                    android:valueFrom="0"
                    android:valueTo="90"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:startOffset="10"
                    android:duration="100"
                    android:valueFrom="0"
                    android:valueTo="-12"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="pivotX"
                    android:startOffset="100"
                    android:duration="100"
                    android:valueFrom="6"
                    android:valueTo="12"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="rotation"
                    android:startOffset="100"
                    android:duration="20"
                    android:valueFrom="90"
                    android:valueTo="180"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="vector">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="alpha"
                android:duration="200"
                android:valueFrom="0.333"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:anim/linear_interpolator"/>
        </aapt:attr>
    </target>
</animated-vector>
